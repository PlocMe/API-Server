/** Footer **/
footer {
    border-top:1px solid #387C90;
    color:#FFF;
    clear:both;
    padding: 10px 2px;
    }

footer a {color:#FFF;}
footer a:hover a, footer a:active, footer a:visited  {color:#EEE;}

.menu-footer {
    float:right;
    list-style: none;
    }

.menu-footer li {
    border-left:1px solid #387C90;
    float:left;
    height:12px;
    line-height:12px;
    margin:0 8px 0 0;
    padding: 0 0 0 8px;
    font-size:90%;
    }
    
.menu-footer li a {text-decoration: none;}
.menu-footer li a:hover {color:#CCC; text-decoration: underline;}
.menu-footer ul {display:none;}

/* The Nivo Slider styles */
.nivoSlider {
	position:absolute;
	z-index:-1;
}
.nivoSlider img {
	position:absolute;
	top:0px;
	left:0px;
}
/* The slices in the Slider */
.nivo-slice {
	display:block;
	position:absolute;
	z-index:1;
	height:100%;
}
/* Caption styles */
.nivo-caption {display:none}
/* Direction nav styles (e.g. Next & Prev) */
.nivo-controlNav, .nivo-directionNav a {display:none}

/*
 * Media queries for responsive design
 * These follow after primary styles so they will successfully override. 
 */

@media all and (orientation:portrait) { 
  /* Style adjustments for portrait mode goes here */
  
}

@media all and (orientation:landscape) { 
  /* Style adjustments for landscape mode goes here */
  
}

/* Grade-A Mobile Browsers (Opera Mobile, iPhone Safari, Android Chrome)  
   Consider this: www.cloudfour.com/css-media-query-for-mobile-is-fools-gold/ */
@media screen and (max-device-width: 480px) {
  
  
  /* Uncomment if you don't want iOS and WinMobile to mobile-optimize the text for you
     j.mp/textsizeadjust 
  html { -webkit-text-size-adjust:none; -ms-text-size-adjust:none; } */
}

/* 
 * print styles
 * inlined to avoid required HTTP connection www.phpied.com/delay-loading-your-print-css/ 
 */
@media print {
  * { background: transparent !important; color: #444 !important; text-shadow: none !important; }
  a, a:visited { color: #444 !important; text-decoration: underline; }
  a:after { content: " (" attr(href) ")"; } 
  abbr:after { content: " (" attr(title) ")"; }
  .ir a:after { content: ""; }  /* Don't show links for images */
  pre, blockquote { border: 1px solid #999; page-break-inside: avoid; }
  thead { display: table-header-group; } /* css-discuss.incutio.com/wiki/Printing_Tables */ 
  tr, img { page-break-inside: avoid; }
  @page { margin: 0.5cm; }
  p, h2, h3 { orphans: 3; widows: 3; }
  h2, h3{ page-break-after: avoid; }
}

